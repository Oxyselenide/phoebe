// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "*.pyc":true,
        "**/__pycache__":true,
        "**/*py[ocw]":true,
        "**/*db":true,
        "venv":true,
        "**/*sqlite3":true
    },
    "python.pythonPath": "${workspaceRoot}/venv/bin/python",
}